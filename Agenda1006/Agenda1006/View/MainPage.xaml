<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Agenda1006.View.MainPage"
             xmlns:views="clr-namespace:Agenda1006.View">
  <!--Pages can be added as references or inline-->
    <TabbedPage.Children>
        <NavigationPage Title="Contactos">
            <x:Arguments>
                <views:ContactoPage />
            </x:Arguments>
            <NavigationPage.Icon>
                <OnPlatform x:TypeArguments="FileImageSource">
                    <On Platform="iOS" Value="tab_feed.png"/>
                </OnPlatform>
            </NavigationPage.Icon>
        </NavigationPage>

    </TabbedPage.Children>

</TabbedPage>
